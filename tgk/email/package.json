{
  "name": "@tgk/email-kinja",
  "version": "1.0.0",
  "description": "Kinja-Enhanced Email Orchestration System with temporal intelligence, correctness scoring, and dynamic priority calibration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "node dist/tests.js",
    "test:watch": "nodemon dist/tests.js",
    "test:integration": "node -e \"require('./dist/examples').runAllExamples()\"",
    "dev": "ts-node examples.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "email",
    "orchestration",
    "ai",
    "kinja",
    "temporal-intelligence",
    "priority-calibration",
    "telegram",
    "automation",
    "learning-system"
  ],
  "author": "TGK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brendadeeznuts1111/alchmenyrun/tree/main/tgk/email"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "jinja2": "^1.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.1"
  },
  "peerDependencies": {
    "@tgk/core": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
